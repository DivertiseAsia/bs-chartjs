{
  "name": "@divertise-asia/bs-chart",
  "version": "2.1.0",
  "scripts": {
    "build": "rescript build",
    "start": "rescript build -w",
    "clean": "rescript clean",
    "server": "moduleserve ./ --port 8000 --spa",
    "setup-test": "npm install bs-platform bs-react-testing-library @glennsl/bs-jest bs-webapi bs-jest-dom",
    "test": "jest --testMatch='**/__tests__/*test.bs.js'",
    "ci-test": "npm test -- --coverage --updateSnapshot",
    "build-doc": "bsdoc build . && bsdoc support-files"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "Divertise Asia",
  "license": "MIT",
  "repository": {
    "type": "https",
    "url": "https://github.com/DivertiseAsia/bs-chartjs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "chart.js": "^3",
    "nan": "^2.14.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.3.0",
    "react-dom": "^17.0.2",
    "rescript": "^9.1.4",
    "rescript-webapi": "^0.1.3"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.7.0",
    "bsdoc": "^6.0.2-alpha",
    "canvas": "^2.6.1",
    "install": "^0.13.0",
    "jest-canvas-mock": "^2.2.0",
    "moduleserve": "github:marijnh/moduleserve#7db414e",
    "npm": "^8.1.4",
    "rescript-dom-testing-library": "^1.15.0",
    "rescript-react-testing-library": "^1.3.0"
  }
}
