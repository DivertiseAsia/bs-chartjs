{
  "name": "@divertise-asia/bs-chart",
  "sources": [
    {
      "dir": "src/Components",
      "subdirs": true
    },
    {
      "dir": "src/Demo",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header",
    "-w -48"
  ],
  "jsx": { "version": 4, "mode": "automatic" },
  "package-specs": [
    {
      "module": "es6",
      "in-source": false
    }
  ],
  "suffix": ".bs.js",
  "namespace": false,
  "bs-dependencies": [
    "@rescript/react"
  ],
  "bs-dev-dependencies": [
    "rescript-dom-testing-library",
    "rescript-react-testing-library"
  ]
}